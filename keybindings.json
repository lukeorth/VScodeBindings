[
    // use Ctrl+n and Ctrl+p to cycle through terminals
    {
        "key": "ctrl+n",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.terminal.focusPrevious",
        "when": "terminalFocus"
    }
]
